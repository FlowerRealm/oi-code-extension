{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out",
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "../constants.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}